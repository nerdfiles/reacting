{
  "name": "reacting",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "diceware": "0.0.1",
    "node-sass": "^7.0.3",
    "pm2": "^5.2.0",
    "random-js": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "require-css": "^0.1.10",
    "tcomb": "^3.2.29",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "fora.new": "n.fora.new",
    "fora": "n.fora",
    "docs": "jsdoc -d docs -r src",
    "docs/open": "open docs/index.html",
    "open/@todo": "nohup vim $(ack @todo -l | percol)",
    "recent": "ack @filepath src -l | percol",
    "percol/alder": "alder src | percol",
    "start": "react-scripts start",
    "publish": "cp -R docs build/docs && cp -R report build/report && echo 'arrested.surge.sh' > ./build/CNAME && cd build && surge .",
    "build": "react-scripts build",
    "build/dev": "NODE_ENV=dev npm run build --dev --configuration=dev",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage",
    "test/sass": "echo 'implement sass-true'",
    "test/behavior": "echo 'implement cypress'",
    "report/open": "open report/index.html",
    "report/complexity": "/node_modules/.bin/es6-plato -r -l .jshintrc -e .eslintrc.backup -d report src",
    "help": "npm run $(echo \"$(npm run)\" | percol)",
    "install/homebrew": "/bin/bash -c \"$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)\"",
    "install/percol": "brew install percol"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "es6-plato": "^1.2.3",
    "gulp": "^4.0.2",
    "jasmine": "^4.4.0",
    "jsdoc": "^3.6.11",
    "jsdoc-api": "^7.1.1",
    "sass": "^1.54.9",
    "sass-true": "^6.1.0",
    "specificity-graph": "^0.1.7"
  }
}
